<div id="qlist-wrapper">
  <div id="question-mini-list">
    <% @questions.each do |question| %>
      <div class="question-summary">
        <div>
          <div class="votes">
            <div class="mini-counts"></div>
            <div>Votes</div>
          </div>
          <div class="status unanswerd">
            <div class="mini-counts"></div>
            <div>answers</div>
          </div>
          <div class="views">
            <div class="mini-counts"></div>
            <div>Views</div>
          </div>
        </div>
        <div class="summary">
          <h3><%= link_to question.title, question_path(question) %></h3>
          <div class="tags">
            
          </div>
          <span class="started"><%= time_ago_in_words(question.created_at) %> ago</span><br/>
        </div>
      </div>
    <% end %>
  </div>
</div>
